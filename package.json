{
  "name": "anneko",
  "version": "0.1.0",
  "description": "A/N Neko",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "commit": "git cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dd-center/ANNeko.git"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": []
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    }
  },
  "author": "Il Harper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dd-center/ANNeko/issues"
  },
  "homepage": "https://github.com/dd-center/ANNeko#readme",
  "dependencies": {
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "git-cz": "^3.2.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.3.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ]
}
